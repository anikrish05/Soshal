{
	"version": 2,
	"builds": [
		{
			"src":"./server.js",
			"use":"@now/node"
		},
		{ 
			"src": "./cronJob.js", 
			"use": "@now/node" 
		}

	],
	"routes":[
		{
			"src":"/(.*)",
			"dest":"server.js"
		}
	],
	"functions": {
    	"cronJob": {
      		"schedule": "33 17 * * *",
      		"maxDuration": 10
    	}
 	 }
}