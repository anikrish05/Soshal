{
	"version": 2,
	"builds": [
		{
			"src":"./server.js",
			"use":"@now/node"
		}
	],
	"functions": {
		"/api/**/**/**": {
			"maxDuration": 60
		}
	},

	"routes":[
		{
			"src":"/(.*)",
			"dest":"server.js"
		}
	],
	"crons": [
		{
			"path": "/api/crons/cronsRepeatable",
			"schedule": "30 11 * * *"
		}
	]

}